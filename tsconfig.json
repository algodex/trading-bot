{
  "compilerOptions": {
    "outDir": "./built",
    "baseUrl": ".",
    "paths": {
      "@": [
        "./"
      ],
      "@/*": [
        "./*"
      ]
    },
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "preserve",
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    "pages/_document.js"
  ],
  "exclude": [
    "node_modules",
    "built/**"
  ]
}
